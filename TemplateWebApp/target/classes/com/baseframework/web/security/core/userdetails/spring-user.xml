<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "/tld/spring-beans-2.0.dtd">
<beans>
	<bean id="userDAO" class="com.baseframework.dao.security.core.userdetails.UserDAOImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	<bean id="userService" class="com.baseframework.biz.security.core.userdetails.UserServiceImpl">
		<property name="md5PasswordEncoder" ref="md5PasswordEncoder" />
		<property name="userDAO" ref="userDAO" />
		<property name="roleDAO" ref="roleDAO" />
	</bean>	
	<bean id="processUser" class="com.baseframework.web.security.core.userdetails.UserAction" scope="prototype" >
		<property name="userService" ref="userService" />
	</bean>
</beans>