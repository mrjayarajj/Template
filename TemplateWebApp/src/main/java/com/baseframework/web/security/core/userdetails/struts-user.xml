<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="com.baseframework.web.security.core.userdetails" namespace="/security/core/userdetails" extends="basic">
	
		<action name="processUser" class="processUser">
			<result name="success">/jsp/base/security/user.jsp</result>
			<result name="onLoad" type="chain">
				<param name="actionName">processUser</param>
				<param name="method">onLoad</param>
			</result>
			<result name="redirect_onLoad" type="redirect">processUser!onLoad.action</result>
		</action>
		
	</package>	
</struts>